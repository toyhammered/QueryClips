<div class="btn-group">
  <button type="button" class="btn btn-info btn-xs dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    Export data as: <span class="caret"></span>
  </button>
  <ul class="dropdown-menu">
    <li>
      <%= link_to "CSV", query_path(@saved_query, format: :csv) %>
    </li>
    <li>
      <%= link_to "JSON", query_path(@saved_query, format: :json) %>
    </li>
  </ul>
</div>

