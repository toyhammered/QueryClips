<div class="row">
  <h1><%= link_to @visualization.name, visualization_path(@visualization) %></h1>
</div>

<div class="row">
  <div class="pull-left">
    <small>
      Created <%= time_ago_in_words(@visualization.created_at) %> ago from
      <%= link_to @visualization.saved_query.name, query_path(@visualization.saved_query) %>.
    </small>
    <div class="clearfix"></div>
    <small>Modified <%= time_ago_in_words(@visualization.updated_at) %> ago.</small>
  </div>
  <div class="form-group form-inline pull-right">
    Share: <input class="form-control" value="<%= visualization_url(@visualization) %>" readonly />
  </div>
</div>



<div class="row">
  <%= render "shared/error_messages", model: @visualization %>
  <%= render "viz" %>
</div>
